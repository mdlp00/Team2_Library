<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="manaSQL">

	<select id="managerLogin" resultType="int" parameterType="manaVO">
	SELECT
			count(*) cnt
	FROM
			Manager
	WHERE
			manager_id = #{id}
            AND manager_pw = #{pw}         
   </select>
         
 </mapper>
 